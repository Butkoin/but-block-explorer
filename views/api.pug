extends layout

block headContent
        title Restful API information

block content
        script(src="/js/api.js")
        h1.h3 Restful API information
        hr
        div(class="row")
           div(class="col-md-4")
               ul(class="nav-item")
                  each item, index in APIS
                      - var itemObj = JSON.stringify(item);
                      - var hrefAction = `javascript:displayAPIInfo('api_info_display', ${itemObj}, ${baseUri})`
                      li
                           a(class="nav-link", href=hrefAction, role="button")
                              span #{item.name} 
           div(class="col-md-8")
               ul(class="list-unstyled" id="api_info_display")